@using Excubo.Blazor.Diagrams.__Internal

@code {
    [CascadingParameter] public Diagram Diagram { get; set; }
    protected override void OnParametersSet()
    {
        Diagram.LinkSelectorArea = @<DefaultLinkSelector Type="Diagram.Links.DefaultType" TypeChanged="Diagram.Links.DefaultTypeChanged" Arrow="Diagram.Links.DefaultArrow" ArrowChanged="Diagram.Links.DefaultArrowChanged" />;
        base.OnParametersSet();
    }
}
